#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,q;
    cin>>n>>q;
    int* a[n];
    for(int i=0;i<n;i++){
        //vector<int> a[i];
        int k;
        cin>>k;
        a[i]=new int[k];
        for(int j=0;j<k;j++){
            int temp;
            cin>>temp;
            a[i][j]=temp;
        }
        
    }
    for(int k=0;k<q;k++){
        int i,j;
        cin>>i>>j;
        cout<<a[i][j]<<endl;
    }
    return 0;
}
